# Task: Change phrase "Don't panic" to phrase "on tap"

phrase = "Don't panic"
plist = list(phrase)
print(phrase)
print(plist)

for i in range(4):
    plist.pop()
plist.pop(0)
plist.remove("'")
plist.pop(3)

plist.insert(2, ' ')
plist.insert(4, 'a')

new_phrase = ''.join(plist)
print(plist)
print(new_phrase)
