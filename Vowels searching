vowels = ['a', 'e','i', 'o', 'u']
word = input('Provide a word to search for vowels:')
found = []

for letter in word:
    if letter in vowels:
        if letter not in found:
            found.append(letter)
for vovel in found:
    print(vovel)

#Ниже поиск гласных в слове с помощью множеств
    
